<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.momo.mapper.RegionMapper">
    <select id="selectAllState">
        select
            state
        from
            tb_region
    </select>

    <select id="selectByState">
        select
            city
        from
            tb_region
        where
            state=#{state}
    </select>

</mapper>