<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.momo.mapper.TermMapper">
    <insert id="insert" parameterType="com.momo.vo.TermVO">
        insert into
            tb_term_st
            (user_id, user_role, term_cd, checked)
        values
            (#{userId},#{role},#{termCd},#{checked})
    </insert>

    <select id="selectAll">
        select
            *
        from
            tb_term
    </select>

</mapper>