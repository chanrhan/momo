<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.momo.mapper.MsgFormMapper">

    <select id="getAllDefaultForm">
        select
            *
        from
            tb_default_msg_form
        order by
            form_id desc
    </select>

    <select id="selectDefaultForm">
        select
            *
        from
            tb_default_msg_form
        where
            form_id=#{formId}
    </select>

    <select id="selectForm">
        select
            *
        from
            tb_msg_form
        where
            form_id=#{formId}
    </select>

</mapper>