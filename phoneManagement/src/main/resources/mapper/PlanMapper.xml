<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.momo.mapper.PlanMapper">
    <select id="select" >
        select
            *
        from
            tb_plan
        where
            ${qs}
    </select>

    <select id="search" parameterType="com.momo.vo.CommonVO">
        select
            *
        from
            tb_plan
        where
            ${andSelect} and
            ${orSearch}
        ${prop}
    </select>

    <select id="getByMap" resultType="java.util.Map">
        select
            *
        from
            tb_plan
        where
            plan_id=#{planId}

    </select>

    <select id="selectAll">
        select
            *
        from
            tb_plan
    </select>
</mapper>